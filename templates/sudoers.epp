<% |
    String[1] $user,
    Array[String[1]] $cmds,
    Array[String[1]] $env_keep,
    Optional[Array[String[0]]] $custom_config,
| -%>
#---
# This file is generated by CodingFuture `cfauth` Puppet module
# DO NOT EDIT!
#---

<% $env_keep.each |$env_var| { -%>
Defaults:<%= $user %>   env_keep+=<%= $env_var %>
<% } -%>

<% if $custom_config { %><%= $custom_config.join("\n") %><% } -%>

<% $cmds.each |$cmd| { -%>
<%= $user %>   ALL=(ALL:ALL) NOPASSWD: <%= $cmd  %>
<% } -%>
