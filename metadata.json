{
  "name": "codingfuture-cfauth",
  "version": "0.9.4",
  "author": "Andrey Galkin",
  "summary": "Configures system access security (users, public keys, SSH daemon and firewall)",
  "license": "Apache-2.0",
  "source": "https://github.com/codingfuture/puppet-cfauth",
  "project_page": "https://github.com/codingfuture/puppet-cfauth",
  "issues_url": "https://github.com/codingfuture/puppet-cfauth/issues",
  "operatingsystem_support": [
    {
